<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/whitesemlogo"
    tools:context=".TelaCod_Only"
    tools:layout_editor_absoluteX="-9dp"
    tools:layout_editor_absoluteY="0dp">

    <Button
        android:id="@+id/buttonEntrarADM"
        android:layout_width="206dp"
        android:layout_height="61dp"
        android:backgroundTint="@color/black"
        android:text="Entrar"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/conteiner_Login1"
        app:layout_constraintVertical_bias="0.244" />

    <ImageView
        android:id="@+id/menuADMLogo"
        android:layout_width="389dp"
        android:layout_height="408dp"
        android:background="@drawable/loginadm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bl4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.409" />

    <View
        android:id="@+id/conteiner_Login1"
        style="@style/Fundo_conteiner_all"
        android:layout_width="355dp"
        android:layout_height="109dp"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bl4"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/email_edit_login3"
        style="@style/AllTexts"
        android:layout_width="319dp"
        android:layout_height="75dp"
        android:drawableRight="@drawable/padlock_icon"
        android:hint="Digite o código único"
        android:inputType="textPassword"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintEnd_toEndOf="@id/conteiner_Login1"
        app:layout_constraintStart_toStartOf="@id/conteiner_Login1"
        app:layout_constraintTop_toTopOf="@id/conteiner_Login1"
        tools:ignore="HardcodedText" />



</androidx.constraintlayout.widget.ConstraintLayout>